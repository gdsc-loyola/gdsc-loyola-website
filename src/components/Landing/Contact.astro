<script>
  import emailjs from "@emailjs/browser";
  import { float } from "../../lib/animationHelpers";

  const clouds = document.querySelector("#contact-clouds");
  const plane = document.querySelector("#contact-plane");

  if (clouds && plane) {
    float(clouds, {
      max: -8,
      min: -5,
      duration: 1.5,
    });
    float(plane, {
      max: -8,
      min: -5,
      duration: 1.5,
    });
  }

  (function () {
    emailjs.init((import.meta as any).env.PUBLIC_EMAILJS_PUBLIC_KEY);
  })();

  const form = document.getElementById("contact-form");
  if (form instanceof HTMLFormElement) {
    const submitBtn = form.querySelector(
      "button[type=submit]"
    ) as HTMLButtonElement;

    const submitBtnContent = submitBtn.querySelector(
      ".btn-content"
    ) as HTMLSpanElement;

    const sendToast = () =>
      (
        document.querySelector("#contact-form-toast-btn") as HTMLButtonElement
      )?.click();

    form?.addEventListener("submit", function (event) {
      event.preventDefault();

      submitBtn.disabled = true;
      submitBtnContent.innerText = "Sending...";

      const formdata = new FormData(form);
      emailjs
        .send(
          "default_service",
          (import.meta as any).env.PUBLIC_EMAILJS_CONTACT_FORM_TEMPLATE_ID,
          {
            from_email: formdata.get("email"),
            subject: formdata.get("subject"),
            message: formdata.get("message"),
          }
        )
        .then(
          function () {
            submitBtn.disabled = false;
            submitBtnContent.innerText = "Submit";
            form.reset();

            window.__PUBLIC__ = {
              ...window.__PUBLIC__,
              contactFormToast: {
                message: "Message successfully sent!",
                success: true,
              },
            };
            sendToast();
          },
          function (error) {
            submitBtn.disabled = false;
            submitBtnContent.innerText = "Submit";
            form.reset();

            window.__PUBLIC__ = {
              ...window.__PUBLIC__,
              contactFormToast: {
                message: "Something went wrong. Please try again.",
              },
            };

            sendToast();
          }
        );
    });
  }
</script>
